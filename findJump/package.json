{
  "name": "find-jump",
  "displayName": "find-jump",
  "description": "Like Ace Jump, quickly move cursor to visible location",
  "version": "0.0.1",
  "publisher": "msafi",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:findJump.activate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "findJump.activate",
        "title": "Trigger Find-Jump"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {}
}
