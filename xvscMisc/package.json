{
  "name": "xvsc-misc",
  "displayName": "xvscMisc",
  "description": "Misc functionality from xvsc",
  "version": "0.0.1",
  "publisher": "mksafi",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:xvscm.openInSourceTree",
    "onCommand:xvscm.deleteToTextBeginning",
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "xvscm.openInSourceTree",
        "title": "Open current git repo in SourceTree"
      },
      {
        "command": "xvscm.deleteToTextBeginning",
        "title": "Delete to the first non-whitespace character"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/shelljs": "^0.7.4",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "shelljs": "^0.7.8"
  }
}
